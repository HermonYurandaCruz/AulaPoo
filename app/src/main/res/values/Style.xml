<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ConteinerLogin">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">260dp</item>
        <item name="android:background">@drawable/caixalogin</item>
        <item name="android:layout_marginRight">40dp</item>
        <item name="android:layout_marginLeft">40dp</item>
    </style>

    <style name="TextoLogin">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/textologin</item>
        <item name="android:padding">15dp</item>
        <item name="android:maxLength">25</item>
        <item name="android:layout_marginRight">40dp</item>
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_marginBottom">20dp</item>
    </style>
<style name="botao">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/botao</item>
        <item name="android:padding">15dp</item>
        <item name="android:maxLength">25</item>
        <item name="android:layout_marginRight">40dp</item>
        <item name="android:layout_marginLeft">40dp</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_marginBottom">20dp</item>
    </style>
</resources>